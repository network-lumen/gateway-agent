{
  "title": "Gateway Indexer",
  "uid": "gateway-indexer",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 2,
  "refresh": "10s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "panels": [
    {
      "type": "stat",
      "title": "Pinned CIDs",
      "gridPos": { "x": 0, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "indexer_pins_current", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "CID rows in DB",
      "gridPos": { "x": 6, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "indexer_db_rows_cids", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Types indexed (total)",
      "gridPos": { "x": 12, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "indexer_types_indexed_total", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Last pins refresh status",
      "gridPos": { "x": 18, "y": 0, "w": 6, "h": 4 },
      "targets": [
        { "expr": "indexer_pins_last_refresh_success", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 0,
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": { "text": "FAILED" },
                "1": { "text": "OK" }
              }
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Types indexed per second",
      "gridPos": { "x": 0, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(indexer_types_indexed_total[5m])",
          "refId": "A",
          "legendFormat": "types/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 4
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Directory expansions & errors",
      "gridPos": { "x": 12, "y": 4, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(indexer_dirs_expanded_total[5m])",
          "refId": "A",
          "legendFormat": "dirs expanded/s"
        },
        {
          "expr": "rate(indexer_dir_expand_errors_total[5m])",
          "refId": "B",
          "legendFormat": "dir expand errors/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 4
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "IPFS range fallbacks",
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(indexer_ipfs_range_ignored_total[5m])",
          "refId": "A",
          "legendFormat": "range ignored/s"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 6
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "stat",
      "title": "Last pins refresh duration",
      "gridPos": { "x": 12, "y": 12, "w": 6, "h": 4 },
      "targets": [
        { "expr": "indexer_pins_last_refresh_duration_ms", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "stat",
      "title": "Last pins refresh age",
      "gridPos": { "x": 18, "y": 12, "w": 6, "h": 4 },
      "targets": [
        { "expr": "time() - indexer_pins_last_refresh_timestamp", "refId": "A" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "decimals": 0
        },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP requests per second",
      "gridPos": { "x": 0, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "rate(indexer_http_requests_total[5m])",
          "refId": "A",
          "legendFormat": "{{method}} {{path}} {{code}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "decimals": 4
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "HTTP duration max (ms)",
      "gridPos": { "x": 12, "y": 16, "w": 12, "h": 8 },
      "targets": [
        {
          "expr": "indexer_http_request_duration_ms_max",
          "refId": "A",
          "legendFormat": "{{method}} {{path}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "decimals": 1
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
